<?xml-stylesheet href="chrome://webrunner/skin/about.css" type="text/css"?>

<!DOCTYPE window SYSTEM "chrome://webrunner/locale/about.dtd">

<dialog xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
        id="about"
        title="&dialog.title;"
	buttonlabelextra1="&bundle.chkupdates;"
        buttons="accept,extra1"
        ondialogextra1="window.close(); checkForUpdates();"
        width="300" height="300">
  <script type="application/x-javascript" src="chrome://webrunner/content/about.js"/>

  <vbox align="center">
    <image src="chrome://webrunner/content/icon128.png"/>
  </vbox>

  <vbox flex="1">
    <label id="version"/>
    <textbox id="useragent" multiline="true" readonly="true" flex="1"/>
  </vbox>

</dialog>
